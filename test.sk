# Add numbers
func add_numbers(a, b) {
    let c = a + b;
    return c;
}

print("6 + 47 = " + add_numbers(6, 47));

# FizzBuzz
func fizzbuzz(num) {
  if num % 15 {
    print("FizzBuzz");
  } else if num % 3 {
    print("Fizz");
  } else if num % 5 {
    print("Buzz");
   } else {
    print(num);
  }    
}

loop (num in range(1, 100)) {
  fizzbuzz(num);
}
